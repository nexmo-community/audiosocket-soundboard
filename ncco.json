[
  {
    "action": "talk",
    "text": "Please wait while we connect you"
  },
  {
    "action": "connect",
    "eventUrl": [ "http://{{host}}/event"],
    "from": "{{cli}}",
    "endpoint": [
    {
      "type": "websocket",
      "uri": "ws://{{host}}/socket",
      "content-type": "audio/l16;rate=16000",
      "headers": {
        "app": "soundboard",
        "cli": "{{cli}}"
        }
      }
    ]}
]
